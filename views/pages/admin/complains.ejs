<% extend('../../admin_layout.ejs') %>
<div id="content-header">
    <div id="breadcrumb"> <a href="/admin" title="Go to Home" class="tip-bottom"><i class="icon-dashboard"></i>
            Dashboard</a> <a href="#">Complains</a> <a href="#" class="current">All Complains</a> </div>
    <h1>List All Complains</h1>
</div>
<div class="container-fluid">
    <hr>
    <div class="row-fluid">
        <div class="span12">
            <div class="widget-box">
                <div class="widget-content nopadding">
                    <div class="widget-box">
                        <div class="widget-title"> <span class="icon"><i class="icon-th"></i></span>
                            <h5>LISTING ALL COMPLAINS</h5>
                        </div>
                        <div class="widget-content nopadding">
                            <table class="table table-bordered data-table">
                                <thead>
                                    <tr>
                                        <th>Complain</th>
                                        <th>DateTime</th>
                                        
                                    </tr>
                                </thead>
                                <tbody>
                                    <% data.forEach(complain => { %>
                                        <tr>
                            
                                            <td><%= complain.val().complain %></td>
                                            <td><%= complain.val().dateTime %> </td>
                                    
                                            <td><a href="/admin/complainDetail?id=<%= complain.val().id %>"
                                                    class="btn btn-block btn-primary">SHOW DETAIL</a></td>
    
                                        </tr>
                                        <% }); %>

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
